{
  "background": {
    "page": "index.html"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "index_popup.html",
    "default_title": "VK Music Player"
  },
  "description": "Проигрыватель музыки из социальной сети Вконтакте",
  "icons": {
    "128": "images/icon.png"
  },
  "options_ui": {
    "chrome_style": false,
    "page": "index.html"
  },
  "key": "",
  "manifest_version": 2,
  "name": "vk-player",
  "permissions": [
    "contextMenus",
    "notifications",
    "storage",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' https://api.vk.com; object-src 'self'",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.2"
}
